{
	"name": "KUWIN Autologin",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Unofficial Kasetsart University WIreless Network autologin for Chrome",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"notifications",
		"storage",
		"<all_urls>"
	],
	"options_page": "settings.html",
	"background": {
		"scripts": ["kuwintools.js", "ext.js"]
	}
}